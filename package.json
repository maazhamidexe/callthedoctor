{
  "name": "callingagent-node",
  "version": "1.0.0",
  "description": "AI-powered doctor call system with OpenAI Realtime API integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "node test-call-trigger.js",
    "test:quick": "node test-call-trigger.js --quick",
    "test:interactive": "node test-call-trigger.js --interactive",
    "test:health": "node test-call-trigger.js --health",
    "dev": "nodemon server.js",
    "frontend": "cd doctor-ui && npm start",
    "install-all": "npm install && cd doctor-ui && npm install"
  },
  "keywords": [
    "ai",
    "doctor",
    "appointment",
    "openai",
    "realtime",
    "websocket",
    "langgraph"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
